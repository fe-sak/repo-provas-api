{
  "name": "repo-provas-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/server.js",
    "prod:seed": "node dist/prisma/seed.js",
    "dev": "fuser -k 5000/tcp ; nodemon --ignore 'tests/*' src/server.ts",
    "dev:load-envs": "dotenv -e .env.development",
    "dev-docker:compose": "docker-compose -f docker-compose.development.yml --env-file .env.development",
    "dev-docker:up": "npm run dev-docker:compose -- up",
    "dev-docker:down": "npm run dev-docker:compose -- down",
    "dev-docker:command": "npm run dev:load-envs npx prisma generate && npm run dev:load-envs npx prisma migrate deploy && npm run dev:load-envs npx prisma db seed && npm run dev:load-envs npm run dev",
    "test:load-envs": "dotenv -e .env.test",
    "test-docker:compose": "docker-compose -f docker-compose.test.yml --env-file .env.test",
    "test:migration:run": "npm run test:load-envs prisma migrate dev && npm run test:load-envs prisma db seed",
    "test-docker:up": "npm run test-docker:compose -- up --build",
    "test-docker:down": "npm run test-docker:compose -- down --remove-orphans",
    "test": "npx dotenv -e .env.test prisma migrate dev && NODE_OPTIONS=--experimental-vm-modules dotenv -e .env.test jest",
    "ci:build": "docker-compose -f docker-compose.test.yml build",
    "ci:test": "docker-compose -f docker-compose.test.yml run repo-provas-test bash -c 'npm run test:migration:run && npm test'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "@types/cors": "^2.8.12",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.1.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "@faker-js/faker": "^6.2.0",
    "dayjs": "^1.11.1",
    "prettier": "^2.6.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.25",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prisma": "^3.12.0",
    "supertest": "^6.2.3",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
